{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "eslint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "NODE_ENV=test playwright test",
    "test:e2e:debug": "NODE_ENV=test PWDEBUG=1 playwright test",
    "test:e2e:codegen": "npx playwright codegen localhost:3000",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "generate:route-types": "cd src && npx nextjs-routes",
    "generate:api": "npx --yes openapi-typescript-codegen --input https://pippams-dev.eoraa.com/apidocs/?format=openapi --output ./src/api --client axios --useOptions --useUnionTypes --exportCore false --exportServices true --exportModels true --exportSchemas false",
    "⭕⭕ THESE ARE TEMPORARY TILL NEXTJS ALLOWS COMPILE-TIME TYPE CHECK ⭕⭕": "https://github.com/vercel/next.js/issues/14997#issuecomment-771109311",
    "dev:ts": "npm run dev & npm run ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "npm run ts --watch",
    "prepare": "husky install"
  },
